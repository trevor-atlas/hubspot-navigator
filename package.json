{
  "name": "hubspot-navigator",
  "version": "1.0.0",
  "description": "Easily navigate the HubSpot UI",
  "main": "index.js",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "__DEV__=true vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.js",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "fuse.js": "^6.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-pattern": "^4.1.4",
    "zustand": "^4.3.3"
  },
  "author": "Trevor Atlas",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.213",
    "@types/jest": "^29.4.0",
    "@types/node": "18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitejs/plugin-react": "3.1.0",
    "babel-loader": "^9.1.2",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "fs-extra": "10.1.0",
    "glob": "^8.1.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup": "3.15.0",
    "sass": "1.58.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vite": "4.1.1",
    "ws": "8.12.1"
  }
}
